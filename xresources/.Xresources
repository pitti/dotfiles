*utf8: 1
*locale: true
*wideChars: true

urxvt*scrollBar: off
urxvt*saveLines: 10000
urxvt*metaSendsEscape: true
urxvt*termName: rxvt-256color
urxvt*urgentOnBell: True

! Clickable URLs
urxvt.perl-ext-common:  default,matcher
urxvt.urlLauncher:      xdg-open
urxvt.matcher.button:   1

urxvt*borderColor:        #151515
urxvt*colorBD:            #ffffff
urxvt*cursorColor:        #ff8939
urxvt*cursorColor2:       #151515


#ifdef EXT_NV_GLX
! Force 96 DPI since the NVIDIA driver will calculate a higher DPI value
! when used in dual-screen setups.
Xft.dpi:       96
#endif

Xft.rgba:       none

! urxvt*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-15
! urxvt*font: xft:Droid Sans Mono Slashed:size=8
! urxvt*font: xft:Source Code Pro:size=10,xft:DejaVu Sans Mono:size=10
urxvt*font: xft:Fantasque Sans Mono:size=11,xft:DejaVu Sans Mono:size=10
! urxvt*font: xft:PT Mono:size=10
! urxvt*font: xft:Consolas:size=8
urxvt.letterSpace: -1

#ifdef XRDB_THEME
#include "current.theme"
#endif

! vim: set filetype=xdefaults :
